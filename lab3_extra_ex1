M= 4
N = 3
a = []
for i in range(N):
    z = []
    for j in range(M):
        z.append(int(input()))
    a.append(z)
 
b = []
for i in range(N):
    z = []
    for j in range(M):
        z.append(int(input()))
    b.append(z)
 
c = b[0:]
for i in range(N):
    for j in range(M):
        if a[i][j] > b[i][j]:
            c[i][j] = a[i][j]
 
print("Первый массив:")
for i in a:
    print(i)
print("Второй массив:")
for i in b:
    print(i)
print("Итоговый массив:")
for i in c:
    print(i)
